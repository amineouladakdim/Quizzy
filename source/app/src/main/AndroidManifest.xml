<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ouladakdimamine.quizzproject">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application

        android:hardwareAccelerated="false"
        android:largeHeap="true"
        android:screenOrientation="portrait"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".Presenteurs.GestionQuizzsActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>

        <meta-data
            android:name="DATABASE"
            android:value="ne12.db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />

        <activity android:name=".Presenteurs.ModelesCRUD.AddQuizActivity" />
        <activity android:name=".Presenteurs.ModelesCRUD.EditQuizActivity" />
        <activity android:name=".Presenteurs.ModelesCRUD.AddCategorieActivity" />
        <activity android:name=".Presenteurs.ModelesCRUD.EditCategorieActivity" />
        <activity android:name=".Presenteurs.ModelesCRUD.AddQuestionActivity" />
        <activity android:name=".Presenteurs.ModelesCRUD.EditQuestionActivity" />
        <activity android:name=".Presenteurs.ModelesCRUD.AddPropositionActivity" />
        <activity android:name=".Presenteurs.ModelesCRUD.EditPropositionActivity" />
        <activity android:name=".Presenteurs.CategorieGameActivity" />
        <activity android:name=".Presenteurs.ResultActivity" />
        <activity android:name=".Presenteurs.QuizActivity" />
<activity android:name=".Presenteurs.QuizGameActivity" />
        <activity android:name=".Presenteurs.HomeActivity">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>